<div class="lands-view dashboard-content">
  <div>
    <p>3. {{user.name}}'s' {{land.name}}</p>
  </div>
  <div class="listen">
    <p>Buy new home.</p>
  </div>
  <a href="/">
    <div class="listen">
      <p>Back to Index.</p>
    </div>
  </a>
</div>

<div class="user-view scroll-content">
  <p>Lots owned by {{user.name}}:</p>
  {{#each homes}}
  <a href="/users/{{../userId}}/lands/{{landId}}/{{this.id}}">
    <div class="listen">
      <p>Click for {{this.name}}</p>
    </div>
  </a>
  {{/each}}
</div>